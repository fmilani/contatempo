{
  "name": "contatempo",
  "version": "0.5.2",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "dev": "TZ=Europe/London meteor --settings settings.json",
    "lint": "eslint . --ext .js,.jsx",
    "prettier":
      "prettier --write --single-quote --trailing-comma all \"client/**/*.js\" \"server/**/*.js\" \"imports/**/*.js\" \"imports/**/*.jsx\"",
    "test":
      "spacejam test --driver-package practicalmeteor:mocha-console-runner --coverage out_lcovonly",
    "tdd":
      "meteor test --driver-package practicalmeteor:mocha --settings settings.coverage.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.41",
    "material-ui": "^0.20.0",
    "meteor-node-stubs": "~0.3.3",
    "moment": "^2.21.0",
    "moment-duration-format": "^2.2.2",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-konami": "^0.5.0",
    "react-router": "^3.2.0",
    "react-tap-event-plugin": "^3.0.2",
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "chai-datetime": "^1.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-meteor": "^4.2.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.11.1"
  }
}
