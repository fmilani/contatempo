{
  "name": "contatempo",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "dev": "TZ=Europe/London meteor --settings settings.json",
    "lint": "eslint . --ext .js,.jsx",
    "prettier":
      "prettier --write --single-quote --trailing-comma all \"client/**/*.js\" \"server/**/*.js\" \"imports/**/*.js\" \"imports/**/*.jsx\"",
    "test":
      "spacejam test --driver-package practicalmeteor:mocha-console-runner --coverage out_lcovonly",
    "tdd":
      "meteor test --driver-package practicalmeteor:mocha --settings settings.coverage.json"
  },
  "dependencies": {
    "material-ui": "^0.18.7",
    "meteor-node-stubs": "~0.2.11",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "moment-timezone": "^0.5.13",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-konami": "^0.4.0",
    "react-router": "^3.2.0",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.23.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai-datetime": "^1.5.0",
    "coveralls": "^2.13.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-meteor": "^0.3.4",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-meteor": "^4.1.6",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^6.10.3",
    "prettier": "^1.7.3"
  }
}
